<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout  xmlns:android="http://schemas.android.com/apk/res/android"
  android:id="@+id/listelement_below"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_below="@+id/listelement_base">

  <View
    android:id="@+id/divider"
    android:layout_width="fill_parent"
    android:layout_height="1dp"
    android:background="#20000000"
    android:layout_alignParentTop="true"
    android:layout_marginLeft="30dp"
    android:layout_marginRight="30dp"/>

  <com.uni_wuppertal.iad.vierteltour.utility.font.CustomFontTextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/addinfo"
    android:lineSpacingExtra="3dp"
    android:textSize="@dimen/tourlist_text"
    android:layout_marginLeft="90dp"
    android:textColor="@color/black"
    android:paddingRight="20dp"
    android:layout_marginTop="10dp"/>
  <ImageButton
    android:layout_width="@dimen/maps_supl_zumstart_width"
    android:layout_height="@dimen/maps_supl_zumstart_height"
    android:id="@+id/zumstartlist"
    android:layout_below="@+id/addinfo"
    android:layout_alignParentRight="true"
    android:scaleType="fitCenter"
    android:paddingRight="@dimen/maps_supl_zumstart_paddingRight"
    android:paddingBottom="10dp"
    android:visibility="invisible"
    android:background="@color/transparent"
    android:onClick="swapToViewPager"/>
</RelativeLayout>
